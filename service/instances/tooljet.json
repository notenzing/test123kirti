{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/service/service.schema.json",
  "provided": false,
  "disabled": false,
  "version": "0.1",
  "metadata": {},
  "flavor": "default",
  "kind": "service",
  "spec": {
    "type": "application",
    "enable_host_anti_affinity": true,
    "release": {
      "image": "tooljet/tooljet-ce:latest",
      "strategy": {
        "type": "Recreate"
      }
    },
    "env": {
      "PG_HOST": "${postgres.name.out.interfaces.writer.host}"
    },
    "runtime": {
      "ports": {
        "http": {
          "port": "3000",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "100m",
        "memory": "800Mi"
      },
      "health_checks": {
        "start_up_time": 10,
        "timeout": 100,
        "period": 10,
        "port": 3000
      },
      "autoscaling": {
        "ram_threshold": 90,
        "max": 10,
        "min": 10
      }
    }
  }
}