{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/mongo/mongo.schema.json",
  "version": "0.2",
  "flavor": "k8s",
  "kind": "mongo",
  "lifecycle": "ENVIRONMENT",
  "disabled": false,
  "provided": false,
  "metadata": {
    "name": "mongo"
  },
  "spec": {
    "authenticated": true,
    "mongo_version": "6.0.2",
    "size": {
      "cpu": "2001m",
      "memory": "900MiB",
      "volume": "8G",
      "instance_count": 3
    }
  }
}