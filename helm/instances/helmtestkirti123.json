{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/helm/helm.schema.json",
  "kind": "helm",
  "flavor": "default",
  "provided": false,
  "disabled": false,
  "metadata": {},
  "version": "0.1",
  "spec": {
    "helm": {
      "chart": "appsmith",
      "repository": "https://helm.appsmith.com",
      "namespace": "test-namespace",
      "wait": false
    },
    "env": [
      {
        "name": "datadog.apiKey",
        "type": "secret",
        "attribute": "DATADOG_API_KEY"
      },
      {
        "name": "datadog.clusterName",
        "type": "static",
        "default": "some_value"
      }
    ],
    "values": {
      "datadog": {
        "service": {
          "annotations": {
            "key": "value"
          }
        }
      }
    }
  }
}